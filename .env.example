# µLM AI Workflow Playground Environment Variables

# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=µLM AI Workflow Playground

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_DEMO_MODE=false
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=true

# Performance Settings
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600000

# Budget Management
VITE_BUDGET_LIMIT=25.00
VITE_BUDGET_WARNING_THRESHOLD=20.00

# Export Settings
VITE_EXPORT_FORMATS=python,docker,huggingface
VITE_DEFAULT_EXPORT_FORMAT=python

# Simulation Settings
VITE_SIMULATION_TIMEOUT=30000
VITE_MAX_SIMULATION_NODES=20

# Development Settings
VITE_DEV_PORT=3000
VITE_DEV_HOST=localhost
VITE_DEV_OPEN=true
